<% if user_signed_in? %>
	<div class="search-form" style="margin-top: 70px; text-align: center;"">
		<%= form_tag(search_path, method: :get) do %>
			<%= text_field_tag 'content' %>
			<%= select_tag 'model',
			options_for_select({ User: "user", Book: "book"}) %>
			<%= select_tag 'how',
			options_for_select({ 完全一致: "match", 前方一致: "forward", 後方一致: "backward", 部分一致: "partical" }) %>
			<%= submit_tag '検索' %>
		<% end %>
	</div>
<% end %>
